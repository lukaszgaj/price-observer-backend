{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": {
      "options": [
        "single",
        "avoid-escape"
      ]
    },
    "interface-name": {
      "options": [
        "never-prefix"
      ]
    },
    "no-namespace": false,
    "no-var-requires": false,
    "arrow-parens": false,
    "member-access": [
      true,
      "no-public"
    ],
    "array-type": [
      true,
      "array"
    ],
    "no-empty-interface": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import "
      }
    ],
    "no-console": false,
    // TODO restore?
    "no-debugger": false
    // TODO restore?
  },
  "rulesDirectory": [
  ]
}